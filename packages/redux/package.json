{
  "name": "@kununu/redux",
  "author": "kununu",
  "license": "ISC",
  "version": "1.0.2",
  "description": "Hosts react actions and reducers to be used cross application.",
  "main": "dist/",
  "scripts": {
    "build": "babel --extensions '.ts,.tsx' src --out-dir dist --ignore 'src/**/*.spec.ts'",
    "clean": "../../scripts/clean.sh",
    "lint": "eslint ./src",
    "beta": "npm publish --tag beta",
    "prepack": "npm run test && npm run build",
    "postpack": "npm run clean",
    "test": "npm run lint && npm run test:jest",
    "test:jest": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@kununu/kununu-utils": "1.27.0",
    "@kununu/utils": "1.1.1",
    "lodash": "4.17.21",
    "react-redux": "7.2.5",
    "redux": "4.1.1",
    "redux-devtools-extension": "2.13.9",
    "redux-mock-store": "1.5.4",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.9"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.15.0",
    "@kununu/eslint-config": "2.0.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/jest": "27.0.1",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "babel-plugin-module-resolver": "4.1.0",
    "jest": "27.1.0",
    "jsdom": "17.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "typescript": "4.4.2"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  }
}
