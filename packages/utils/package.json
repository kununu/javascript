{
  "name": "@kununu/utils",
  "author": "kununu",
  "license": "ISC",
  "version": "1.0.0-beta.7",
  "description": "Hosts react utils to be used cross application.",
  "main": "dist/",
  "scripts": {
    "build": "babel --extensions '.ts,.tsx' src --out-dir dist --ignore 'src/**/*.spec.ts'",
    "clean": "../../scripts/clean.sh",
    "lint": "eslint ./src",
    "beta": "npm publish --tag beta",
    "prepack": "npm run test && npm run build",
    "postpack": "npm run clean",
    "test": "npm run lint && npm run test:jest",
    "test:jest": "NODE_ENV=test jest"
  },
  "dependencies": {
    "is-mobile": "3.0.0",
    "@kununu/kununu-utils": "1.25.3",
    "lodash": "4.17.21",
    "camelize": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.15.0",
    "@kununu/eslint-config": "2.0.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@types/jest": "26.0.23",
    "@types/react": "17.0.5",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "jest": "26.6.3",
    "jsdom": "16.5.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "typescript": "4.3.4",
    "fetch-mock": "9.11.0"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "isomorphic-fetch": ">=3.0.0",
    "qs": ">=6.10.1",
    "http-status-codes": ">=2.1.4",
    "next": "9.x"
  }
}
